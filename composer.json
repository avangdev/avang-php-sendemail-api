{
    "name": "avangdev/avang-php-sendemail-api",
    "type": "library",
    "description": "PHP API for send Email. Easily send outgoing email",
    "keywords": ["AvangEmail"],
    "homepage": "https://www.avangemail.com",
    "autoload": {
        "psr-4": {
            "AvangPhpApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AvangPhpApi\\Test\\": "tests/"
        }
    },
    "require": {
